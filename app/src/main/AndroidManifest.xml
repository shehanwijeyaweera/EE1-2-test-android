<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ee1_2_test">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EE12test">
        <activity android:name=".Activity.Userhomepage"
            android:theme="@style/Theme.EE12test.NoActionbar"
            android:label="OnlineBookStore"></activity>
        <activity android:name=".Activity.userSingleBookView" />
        <activity android:name=".Activity.viewAllBooksUser" />
        <activity android:name=".Activity.StoreworkerDashboard" />
        <activity android:name=".Activity.AdminDashboard" />
        <activity android:name=".Activity.Single_book_view" />
        <activity android:name=".Activity.DisplayBooks" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/network_security_config" />

        <activity android:name=".Activity.MainActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>